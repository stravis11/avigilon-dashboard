# Avigilon ACC Configuration
# Copy this file to .env and fill in your values

# ACC Server Connection
ACC_SERVER_URL=https://your-acc-server:8443
ACC_USERNAME=your_acc_username
ACC_PASSWORD=your_acc_password
ACC_USER_NONCE=your_user_nonce
ACC_USER_KEY=your_user_key
ACC_CLIENT_NAME=AvigilonWebApp

# CORS Configuration (comma-separated list of allowed origins)
ALLOWED_ORIGINS=http://localhost,http://localhost:80
